{{header-component}}
<div class="container-fluid">
<div class="well jumbo-color">
	<div class="row">
    	<div class="col-md-12 ">
           	<div class="panel panel-default">
	            <div class="panel-heading">
	            	<h3 class="panel-title">Ver Solicitudes</h3>
	            </div>
               	<div class="panel-body">
               		<div class="row">
               			<div class="col-md-12  text-center">
				           	<div class="panel panel-default">
				               	<div class="panel-body">
				               		<div class="row">
				               			<div class="col-md-12  text-center">
				               				<button class="btn btn-lg width-80" type="submit" {{action 'solicitudes'}}>
				               				<span class="pull-left" >{{fa-icon "file-text-o"}}</span> 
				               				Solicitudes
				               				</button>
				               				
				               			</div>
				               		</div>
				                </div>
				            </div>
               			</div>
               		</div>
                </div>
            </div>
        </div>

        <div class="col-md-12 ">
           	<div class="panel panel-default">
	            <div class="panel-heading">
	            	<h3 class="panel-title">Ver Proyectos</h3>
	            </div>
               	<div class="panel-body">
               		<div class="row">
               			<div class="col-md-12  text-center">
	           				<div class="panel panel-default">
				               	<div class="panel-body">
				               		<div class="row">
				               			<div class="col-md-12  text-center">
				               				<button class="btn btn-lg width-80" type="submit" {{action 'proyectos'}}>
				               				<span class="pull-left" >{{fa-icon "folder-open-o"}}</span> 
				               				Proyectos
				               				</button>
				               				
				               			</div>
				               		</div>
				                </div>
				            </div>
	           			</div>
               		</div>
                </div>
            </div>
        </div>
    </div>
</div>

